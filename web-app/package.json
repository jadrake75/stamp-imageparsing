{
    "name": "stamp-imageparsing",
    "productName": "Stamp Image Bursting Application",
    "description": "Next-Generation Image Bursting Application",
    "version": "3.0.0",
    "bugs": {
        "url": "https://github.com/jadrake75/stamp-imageparsing/issues"
    },
    "repository": "github:jadrake75/stamp-imageparsing",
    "license": "Apache 2.0",
    "author": "Jason Drake (jadrake75@gmail.com)",
    "main": "main.js",
    "scripts": {
        "build": "au build",
        "watch": "au run --watch",
        "prewin32": "au build",
        "win32": "build --x64",
        "start": "electron .",
        "postinstall": "electron-builder install-app-deps",
        "rebuild-modules": "electron-rebuild && node node_modules/java/postInstall.js",
        "package-win": "electron-packager . stamp-imageparsing --overwrite --platform=win32 --arch=x64 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Stamp Image Bursting Application\""
    },
    "build": {
        "appId": "com.drakeserver.stamp-imageparsing",
        "copyright": "Copyright ï¿½ 2019 Jason Drake (jadrake75@gmail.com)",
        "win": {
            "icon": "assets/icons/win/icon.ico",
            "target": "nsis",
            "files": [
                "assets/fonts/stamp-web/css/stamp-web-embedded.css",
                "assets/images${/*}",
                "assets/svg${/*}",
                "LICENSE",
                "main.js",
                "index.html",
                "scripts${/*}",
                "lib{/*}",
                "node_modules/bootstrap/dist/css/bootstrap.min.css",
                "platform${/*}",
                "locales${/*}",
                "!assets/sketches",
                "!build${/*}",
                "!test${/*}",
                "!target${/*}",
                "!node_modules/aurelia*/${/*}",
                "!node_modules/bootstrap/js/${/*}",
                "!node_modules/bootstrap/scss/${/*}",
                "!node_modules/bootstrap/dist/js/${/*}",
                "!node_modules/bootstrap/dist/css/bootstrap-grid*",
                "!node_modules/bootstrap/dist/css/bootstrap-reboot*",
                "!node_modules/bootstrap/dist/css/bootstrap-css*",
                "!nbproject${/*}",
                "!aurelia_project${/*}",
                "!java${/*}",
                "!build.xml",
                "!pom.xml",
                "!gulpfile.js",
                "!karma.conf.js",
                "!.babelrc",
                "!.editorconfig",
                "!.eslintrc.json",
                "!.gitignore",
                "!src${/*}"
            ]
        },
        "asar": true,
        "asarUnpack": [
            "lib/*"
        ],
        "icon": "assets/icons/win/icon.ico",
        "nsis": {
            "oneClick": false,
            "allowToChangeInstallationDirectory": true,
            "artifactName": "stamp-imageparsing-install.${ext}",
            "installerSidebar": "images/sidebar.bmp",
            "installerLanguages": [
                "en-US"
            ]
        },
        "directories": {
            "output": "release-builds",
            "buildResources": "assets"
        },
        "extraFiles": [
            "lib/**"
        ]
    },
    "dependencies": {
        "@stomp/stompjs": "github:stomp-js/stompjs",
        "aurelia-fetch-client": "^1.8.2",
        "bluebird": "^3.4.1",
        "bootstrap": "^4.1.3",
        "changedpi": "^1.0.3",
        "http-proxy-middleware": "^0.19.1",
        "jquery-sparkline": "^2.4.0",
        "lodash": "^4.17.15",
        "requirejs": "^2.3.2",
        "sharp": "^0.20.8",
        "sockjs-client": "^1.3.0",
        "text": "github:requirejs/text#latest",
        "uid-generator": "^2.0.0",
        "url": "^0.11.0",
        "when": "^3.7.8"
    },
    "devDependencies": {
        "aurelia-animator-css": "^1.0.1",
        "aurelia-bootstrapper": "^2.1.0",
        "aurelia-cli": "^1.1.0",
        "aurelia-dialog": "^2.0.0-rc.3",
        "aurelia-i18n": "2.3.1",
        "aurelia-testing": "^1.0.0-beta.2.0.1",
        "aurelia-tools": "^1.0.0",
        "babel-eslint": "^6.0.4",
        "babel-plugin-syntax-flow": "^6.8.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-es2015-modules-amd": "^6.8.0",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
        "babel-plugin-transform-flow-strip-types": "^6.8.0",
        "babel-polyfill": "^6.9.1",
        "babel-preset-es2015": "^6.13.2",
        "babel-preset-stage-1": "^6.5.0",
        "babel-register": "^6.9.0",
        "browser-sync": "^2.26.3",
        "electron": "^3.0.7",
        "electron-builder": "^20.28.4",
        "electron-packager": "^12.1.0",
        "electron-rebuild": "^1.8.2",
        "electron-winstaller": "^2.6.4",
        "gulp": "4.0.0",
        "gulp-babel": "^6.1.2",
        "gulp-changed-in-place": "^2.0.3",
        "gulp-eslint": "^2.0.0",
        "gulp-htmlmin": "^3.0.0",
        "gulp-notify": "^3.2.0",
        "gulp-plumber": "^1.1.0",
        "gulp-rename": "^1.2.2",
        "gulp-sass": "3.1.0",
        "gulp-shell": "^0.6.5",
        "gulp-sourcemaps": "^2.0.0-alpha",
        "handlebars": "^4.5.3",
        "html-minifier": "^3.5.19",
        "http-proxy-middleware": "^0.19.1",
        "i18next": "11.3.3",
        "i18next-xhr-backend": "1.5.1",
        "jasmine-core": "^2.4.1",
        "jquery": "^3.5.0",
        "karma": "^4.2.0",
        "karma-babel-preprocessor": "^6.0.1",
        "karma-chrome-launcher": "^1.0.1",
        "karma-coverage": "^1.1.2",
        "karma-jasmine": "^1.0.2",
        "karma-junit-reporter": "^1.2.0",
        "karma-phantomjs-launcher": "^1.0.4",
        "karma-remap-istanbul": "^0.6.0",
        "minimatch": "^3.0.2",
        "popper.js": "^1.14.4",
        "require-dir": "^1.0.0",
        "through2": "^2.0.1",
        "uglify-js": "^2.6.3",
        "url": "^0.11.0",
        "vinyl-fs": "^3.0.3"
    }
}
